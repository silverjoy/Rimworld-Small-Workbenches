<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="Silj_MiniBenchBase"> <!-- I read it <3 -->
    <defName>EPOE_TableBionicsMini</defName>
    <label>small bionics workbench</label>
    <thingClass>Building_WorkTable</thingClass>
    <description>A workbench equipped for production of high tech bionics. Provides optimized workspace.</description>
    <graphicData>
      <texPath>Things/Buildings_EPOE/EPOE_TableBionicsMini</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(2.5,1.5)</drawSize>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <costList>
       <Steel>250</Steel>
       <Plasteel>150</Plasteel>
       <Component>3</Component>
    </costList>
    <altitudeLayer>Building</altitudeLayer>
    <fillPercent>0.5</fillPercent>
    <useHitPoints>True</useHitPoints>
    <statBases>
      <WorkToBuild>3750</WorkToBuild>
      <MaxHitPoints>200</MaxHitPoints>
      <Flammability>1.0</Flammability>
    </statBases>
    <size>(2,1)</size>
    <!--
    <designationCategory>Production</designationCategory>
    -->
    <passability>PassThroughOnly</passability>
    <pathCost>70</pathCost>
    <hasInteractionCell>True</hasInteractionCell>
    <interactionCellOffset>(0,0,-1)</interactionCellOffset>
    <surfaceType>Item</surfaceType>
    <!--
    <recipes>
      <li>CreatePowerArm</li>
      <li>CreateBionicArm</li>
      <li>CreateBionicLeg</li>
      <li>CreateBionicHand</li>
      <li>CreateBionicFoot</li>
      <li>CreateBionicSpine</li>
      <li>CreateBionicEye</li>
      <li>CreateBionicEar</li>
      <li>CreateBionicJaw</li>
      <li>CreateSilentJaw</li>
      <li>CreateMedicalRib</li>
      <li>CreateDruggedRib</li>
      <li>CreateAdrenalineRib</li>
      <li>CreatePainkillerRib</li>
    </recipes>
    -->
    <inspectorTabs>
      <li>ITab_Bills</li>
    </inspectorTabs>
    <comps>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <startElectricalFires>true</startElectricalFires>
        <basePowerConsumption>450</basePowerConsumption>
      </li>
      <li Class="CompProperties_Flickable"/>
      <li Class="CompProperties_AffectedByFacilities">
        <linkableFacilities>
          <li>ToolCabinet</li>
        </linkableFacilities>
      </li>
      <li Class="CompProperties_Breakdownable"/>
    </comps>
    <building>
      <spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
    </building>
    <constructEffect>ConstructMetal</constructEffect>
    <!--
    <researchPrerequisites>
      <li>Bionics</li>
    </researchPrerequisites>
    -->
    <placeWorkers>
      <li>PlaceWorker_ShowFacilitiesConnections</li>
    </placeWorkers>
  </ThingDef>

